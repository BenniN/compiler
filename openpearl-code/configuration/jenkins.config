#

#
# Compiler
#
CONFIG_COMPILER_ANTLR="/usr/local/lib/antlr-4.5-complete.jar"

#
# Runtime System
#

#
# Linux
#
CONFIG_LINUX=y
CONFIG_LINUX_Raspberry=y
CONFIG_LINUX_Raspberry_Version_Auto=y
# CONFIG_LINUX_Raspberry_Version_V1B is not set
# CONFIG_LINUX_Raspberry_Version_V2B is not set
# CONFIG_LINUX_Raspberry_Version_V3B is not set
# CONFIG_LINUX_tests is not set

#
# Drivers
#

#
# The standard drivers are build automatically 
#

#
# --> StdStream, Disc, Pipe                    
#

#
# as well as i2c driver and devices            
#
# CONFIG_LINUX_PCAN is not set

#
# LPC1768
#
CONFIG_LPC1768=y
CONFIG_LPC1768_CHECK_STACK_OVERFLOW=y
# CONFIG_LPC1768_tests is not set

#
# Drivers
#
# CONFIG_LPC1768_HAS_USB is not set
# CONFIG_HAS_FAT is not set
# CONFIG_HAS_SDCARDSPI is not set
# CONFIG_LPC1768_USB_Keyboard is not set
# CONFIG_LPC1768_USB_Disk is not set
# CONFIG_LPC1768_SDCARD is not set
CONFIG_LPC1768_UART=y
CONFIG_LPC1768_HY32A=y
CONFIG_CONFIG_LPC1768_FLASHINTERFACE="/dev/ttyUSB0"

#
# ESP32
#
CONFIG_ESP32=y
CONFIG_ESP32_IDF_PATH="~/esp/esp-idf"
CONFIG_ESP32_CHECK_STACK_OVERFLOW=y
CONFIG_ESP32_FLASH_INTERFACE="/dev/ttyUSB0"
CONFIG_DOC_PREPORT=y

#
# Installation Settings
#
CONFIG_INSTALL_Target="/usr/local "
